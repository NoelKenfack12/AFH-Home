<div class="totop"><i class="fa fa-angle-up"></i> To Top</div>
<script>
	$('.totop').tottTop({
		scrollTop: 100
	});
	
	var animtail = 0;
	$('.hidden-menu-right').click(function(){
	  if(animtail == 300)
	  {
		  $( ".side-nav" ).animate({ "left": "+=300px"}, "slow", function() {
			  $('.panel-result-action-hosting-user').hide();
			  $('.content-alert-action-hosting-user').hide();
		  });
		  animtail = 0;
	  }
	});
	
	$('.show-menu-right').click(function(){
	  openModalRight('document');
	});
	
	function openModalRight(opapp)
	{
		$( ".side-nav" ).show();
		  var height = ($(window).height() + $(window).scrollTop());
		  var width = $(window).width() + 100;
		  var scrolltop = $(window).scrollTop();
		  $('.panel-result-action-hosting-user').show();
		  $('.content-alert-action-hosting-user').show();
		  $('.content-alert-action-hosting-user').html('<div style="position: fixed; top: 0px; margin-bottom: '+scrolltop+'px; left: 0px; z-index: 9000; width: '+width+'px; height: '+height+'px; background: rgba(0,0,0,0.5);"></div>');

		  if(animtail == 0)
		  {
			  $( ".side-nav" ).animate({ "left": "-=300px" }, "slow" );
			  animtail = 300;
		  }
		  requestCountryApp(opapp);
	}

var itemapp = 0;
function requestCountryApp(itemapp)
{
	itemapp = itemapp;
	$('.liste-country-application').html('<div class="col-lg-12 text-center"><div style="font-size: 15px; margin-top: 20px; padding: 7px 20px;">Patientez pendant le chargement des pays ... </div><img src="{{ asset('template/images/loader.gif') }}" alt=""></div>');
    $.ajax({
		url : '{{ path('users_localisationuser_chargement_pays_appli') }}',
		type : 'POST',
		dataType : 'html', // On désire recevoir du HTML
		data : "itemapp=" + itemapp,
		success : function(data, statut){ // code_html contient le HTML renvoyé
		  $('.liste-country-application').html(data);
		},
        error: function(resultat, statut, erreur){
            $(".liste-country-application").html('<div class="attente-resultat-ajax-country" style="padding-top: '+80+'px; height: 200px; text-align: center; vertical-align: center; width: 100%; background: #fff; margin-top: 7px;"><div>Erreur de chargement ! </br><a href="#!" onclick="rechargementCountryApp()"><span class="fa fa-repeat"></span> Recharger</a></div></div>');
	   }
	});
}

function rechargementCountryApp(){
    $('.attente-resultat-ajax-country').replaceWith('');
    requestCountryApp(itemapp);
}
</script>

<script type="text/javascript">
$(document).ready(function(){

$('.popover-second').hover(function(){
  $(this).find('.popover2').stop(true, true).delay(200).fadeIn(200);
  }, function(){
  $(this).find('.popover2').stop(true, true).delay(200).fadeOut(200);
});  
  
  $('a.page-scroll-cible').click(function(){
	if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
	  var target = $(this.hash);
	  target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
	  if (target.length) {
		$('html,body').animate({
		  scrollTop: target.offset().top - 40
		}, 900);
		return false;
	  }
	}
	});

});
</script>

