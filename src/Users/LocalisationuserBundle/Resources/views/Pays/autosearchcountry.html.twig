<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1"/>
<meta name="robots" content="index, follow"/>
<title>Africexplorer</title>

<meta name="author" content="Agricexplorer"/>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
<meta charset="UTF-8"/>
<meta name="language" content="French"/>
<link rel="stylesheet" href="{{ asset('ladingpage/carousel/css/style.css') }}"/>
<script type="text/javascript" src="{{ asset('ladingpage/js/jquery.min.js?ver=4.3.1') }}"></script>
<style>
.item:hover{
  box-shadow: 0 8px 17px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.15);
}
</style>
</head>

<body>

<div class="clearfix" style="margin-top: 1px solid #f4f4f4;"></div>
<div class="resCarousel" data-items="2,4,5,6" data-slide="1">
<div class="resCarousel-inner">
	{% for pays in liste_pays %}
		<div class="item" style="background: #fff;">
			<div class="col-xs-12 col-sm-12 col-lg-12 EiProdImg">
				<a href="" target="_blank"><img src="{% if pays.drapeau != null %}{{ asset(pays.drapeau.getwebpath) }}{% else %}{{ asset('ladingpage/images/6890733-mountain-peaks-wallpaper-hd_PZTQDff.jpg') }}{% endif %}" class="img-responsive" style="width: 100%; height: 70px;"/></a>
			</div>
			<div class="col-xs-12 col-sm-12 col-lg-12 EiProdName">
				<div class="EiPName" style="white-space: nowrap;">{{ pays.nom }}</div>
				<div class="EiDisc link-country-size" style="white-space: nowrap;"><a href="{{ pays.siteweb }}" target="_blank">{{ pays.siteweb }}</a></div>
			</div>
		</div>
	{% else %}
		{% for i in range(1,6) %}
		<div class="item" style="background: #fff;">
			<div class="col-xs-12 col-sm-12 col-lg-12 EiProdImg">
				<a href=""><img src="{{ asset('ladingpage/images/6890733-mountain-peaks-wallpaper-hd_PZTQDff.jpg') }}" class="img-responsive" style="width: 100%; height: 70px;"/></a>
			</div>
			<div class="col-xs-12 col-sm-12 col-lg-12 EiProdName">
				<div class="EiPName" style="white-space: nowrap;">Pays - Domaine</div>
				<div class="EiDisc link-country-size" style="white-space: nowrap;"><a href="http://cm.africexplorer.com" target="_blank">www.cm.africexplorer.com</a></div>
			</div>
		</div>
		{% endfor %}
	{% endfor %}
</div>
<button class="btn btn-default leftLst pull-left" style="position: relative; margin-top: -150px; background: #fff; color: #333;"><i class="fa fa-fw fa-angle-left"></i></button>
<button class="btn btn-default rightLst pull-right" style="position: relative; margin-top: -150px; background: #fff; color: #333;"><i class="fa fa-fw fa-angle-right"></i></button>
</div>
<div class="clearfix" style="padding: 7px; background: #fff;"></div>

<script type="text/javascript" src="{{ asset('ladingpage/carousel/js/main.js') }}"></script>
{% if position == 'other' %}
<!-- script src="{{ asset('ladingpage/js/popper.min.js') }}"></script -->
<script src="{{ asset('ladingpage/js/bootstrap.min.js') }}"></script>
{% endif %}
<script src="{{ asset('ladingpage/js/bootstrap-notify.js') }}"></script>
<!--script type='text/javascript' src="{{ asset('ladingpage/js/init.js') }}"></script -->
</body>
</html>