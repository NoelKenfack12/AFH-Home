<div class="row" style="margin-top: 0px;">
	<div class="col-md-12" style="padding: 0px 30px;">
	<div class="resCarousel" data-items="2,2,3,4" data-slide="1" style="padding-left: 0px; padding-right: 0px;">
	<div class="resCarousel-inner" style="max-height: 150px;">
		{% set lastyear = null %}
		{% for annee in liste_annee %}
		<div class="item open-stat-current-year open-stat-current-year-{{ annee.id }}  desactivestatyear {% if loop.first %}{% set lastyear = annee %} activestatyear {% endif %}" value="{{ annee.id }}">
			<div class="col-xs-12 col-sm-12 col-lg-12 EiProdImg">
				<a href="#!"><img src="{% if annee.imgservice != null %}{{ asset(annee.imgservice.getwebpath) }}{% else %}{{ asset('templateafh/images/600.jpg') }}{% endif %}" class="img-responsive" style="width: 100%; height: 70px;"/></a>
			</div>
			<div class="col-xs-12 col-sm-12 col-lg-12 EiProdName">
				<div class="EiPName" style="white-space: nowrap;">
					<strong>{{ annee.nom }}</strong>
				</div>
				<div class="" style="white-space: nowrap;">{{ annee.description }}</div>
			</div>
		</div>
		{% endfor %}
	</div>
	<button class="btn btn-default leftLst pull-left" style="position: relative; margin-top: -130px; z-index: 1; background: #fff; color: #333;"><i class="fa fa-fw fa-angle-left"></i></button>
	<button class="btn btn-default rightLst pull-right" style="position: relative; margin-top: -130px; z-index: 1; background: #fff; color: #333;"><i class="fa fa-fw fa-angle-right"></i></button>
	</div>
	</div>
</div>

