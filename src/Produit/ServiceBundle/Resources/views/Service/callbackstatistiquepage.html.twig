		{{ render(controller("ProduitServiceBundle:Service:applicationstat", {'position': 0, 'anneeid': lastyear.id})) }}
		
		<script type="text/javascript">
			setTimeout(function() { uploadSlideCatalogue({{ lastyear.id }}); }, 500);
		</script>
	{% else %}
		<h2 style="text-align: center; margin-top: 20px;">Aucune Application disponible pour le moment ...</h2>
	{% endif %}
</div>

<script type="text/javascript" src="{{ asset('template/carousel/js/main.js') }}"></script>

</div>
