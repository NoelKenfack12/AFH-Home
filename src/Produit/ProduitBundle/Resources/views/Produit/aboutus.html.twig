<script type="text/javascript">
	function initMap() {
	var uluru =  {lat: 3.878644381032418, lng: 11.547444450514263};
	var map = new google.maps.Map(document.getElementById('map-canvas'), {
	  zoom: 17,
	  center: uluru,
	  scrollwheel: false
	});

	var image = '{{ asset('newtemplate/images/icons/africexplorer.png') }}';
		image = {
			  url: image,
			  size: new google.maps.Size(200, 200),
			  origin: new google.maps.Point(0, 0),
			  anchor: new google.maps.Point(0, 0),
			  scaledSize: new google.maps.Size(100,100)
			};	
		var currentmark = new google.maps.Marker({
		  position: uluru,
		  map: map,
		  hide: false,
		  icon: image,
		  animation: google.maps.Animation.DROP
		});
		
		var infowindow = new google.maps.InfoWindow();
		google.maps.event.addListener(currentmark, 'mouseover', function(){
        infowindow.setContent('<div>La direction générale de Africexplorer est ici</br>venez discuter de votre projet avec nos techniciens !</div>');
        infowindow.open(map, this);
        });

		map.panTo(new google.maps.LatLng(uluru));
	}  
	</script>
